{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~/*": [
                "./*"
            ],
            "@/*": [
                "./*"
            ],
            "~~/*": [
                "./*"
            ],
            "@@/*": [
                "./*"
            ]
        },
        "module": "esnext",
        "moduleResolution": "node",
        "target": "esnext",
        "lib": [
            "esnext",
            "dom"
        ]
    },
    "exclude": [
        "node_modules",
        ".nuxt",
        "dist"
    ]
}